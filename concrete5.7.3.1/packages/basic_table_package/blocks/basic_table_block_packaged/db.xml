<?xml version="1.0"?>
<schema version="0.3">
	<table name="btBasicTableInstance">
		<field name="bID" type="I">
			<key />
			<unsigned />
			<autoincrement/>
		</field>
		<field name="question" type="C" size="255">
			<default value=""/>
		</field>
		<field name="requiresRegistration" type="I">
			<default value="0"/>
		</field>
	</table>

	<table name="btBasicTable">
		<field name="id" type="I">
			<key/>
			<unsigned/>
			<autoincrement/>
		</field>
		<field name="value" type="C" size="255">
			<unsigned/>
			<default value=""/>
		</field>
	</table>




	<table name ='btBasicTableOption2Group'>
		<field name="ID" type="I">
			<key />
			<unsigned />
			<autoincrement/>
		</field>
		<field name='optionID' type='I'></field>
		<field name='groupID' type='I'></field>
	</table>
</schema>



